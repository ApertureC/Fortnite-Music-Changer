<Window x:Class="Fortnite_Music_WPF.ViewImage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fortnite_Music_WPF"
        mc:Ignorable="d"
        Title="ViewImage" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="text" HorizontalAlignment="Left" Margin="10,377,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="309" Height="32"><Run Text="TextBlock"/></TextBlock>
        <Image x:Name="image" HorizontalAlignment="Left" Height="362" Margin="10,10,0,0" VerticalAlignment="Top" Width="772"/>
        <Button x:Name="Ok" Content="Ok" HorizontalAlignment="Left" Margin="707,389,0,0" VerticalAlignment="Top" Width="75" Click="Ok_Click"/>
        <Button x:Name="Retake" Content="Retake" HorizontalAlignment="Left" Margin="627,389,0,0" VerticalAlignment="Top" Width="75" Click="Retake_Click"/>

    </Grid>
</Window>
